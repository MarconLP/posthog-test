{
  "rewrites": [
    {
      "source": "/ingest/:path*",
      "destination": "https://app.posthog.com/:path*"
    },
    {
      "source": "/ingest/:path(.*)",
      "destination": "https://app.posthog.com/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://acme.com/blog"
    }
  ]
}